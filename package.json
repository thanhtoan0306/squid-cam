{
  "name": "squid-cam",
  "version": "1.0.0",
  "description": "A sample Electron app with two windows",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.yourdomain.squidcam",
    "productName": "SquidCam",
    "files": [
      "**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "nsis"
    }
  },
  "keywords": [
    "Electron",
    "multi-window",
    "demo"
  ],
  "author": "Thanh Toan",
  "license": "MIT",
  "devDependencies": {
    "electron": "^29.0.0",
    "electron-builder": "^26.0.12",
    "electron-reload": "^2.0.0-alpha.1"
  },
  "dependencies": {
    "@mediapipe/tasks-vision": "^0.10.22-rc.20250304",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "phaser": "^3.90.0",
    "socket.io": "^4.8.1",
    "tiktok-live-connector": "^2.0.5-beta1"
  }
}
